.MODEL SMALL
.DATA
 STR DB â€œCHETHAN"
 ARRAY DB 09H, 08H, 07H, 05H, 03H
 COUNT DW 0005H
.CODE
 MOV AX,@DATA
 MOV DS, AX
 MOV BX, OFFSET ARRAY
 MOV CX, COUNT
 DEC CX
NXTSCN: MOV BX, CX
 MOV SI, 0000H
NXTCOP: MOV AL, ARRAY [SI]
 MOV DL, ARRAY [SI+1]
 CMP AL, DL
 JC NOSWP//N
 MOV ARRAY [SI], DL
 MOV ARRAY [SI+1], AL
NOSWP: INC SI
 DEC BX
 JNZ NXTCOP
 LOOP NXTSCN
 MOV AH, 4CH
 INT 21H
 END
