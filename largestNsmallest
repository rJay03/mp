.MODEL SMALL
.DATA
 ARRAY DB 02H, 06H, 0AH, 07H, 09H
 COUNT DB 05H
 RESULT DB ?
.CODE
 MOV AX,@DATA
 MOV DS, AX
 MOV AX, 0000H
 MOV CL, COUNT
 DEC CL
 LEA SI, ARRAY
 MOV AL, [SI]
LOOP1: CMP AL, [SI+1]
 JA LOOP2
 MOV AL, [SI+1]
LOOP2: INC SI
 DEC CL
 JNZ LOOP1
 MOV RESULT, AL
 MOV AH, 4CH
 INT 21H
 END
